package leetCodeEasy;

import java.util.Arrays;

public class twoStringPermutationCheck {

	public static void main(String[] args) {
		String one = "praveen";
		String two = "arpenve";
		char[] arrOne = one.toCharArray();
		char[] arrTwo = two.toCharArray();
		Arrays.sort(arrOne);
		Arrays.sort(arrTwo);
		System.out.println(String.valueOf(arrOne)+" "+arrTwo);
		System.out.println(String.valueOf(arrOne).equals(String.valueOf(arrTwo))?Boolean.TRUE:Boolean.FALSE);

	}

}
